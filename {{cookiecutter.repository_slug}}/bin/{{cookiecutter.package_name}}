#!/usr/bin/env bash

## \brief {{cookiecutter.repository_description}}

shellm-source shellm/doc

if [ $# -eq 0 ]; then
  doc --print-usage "$0"
  exit 1
fi

main() {
  while [ $# -ne 0 ]; do
    case "$1" in
      ## \option -h, --help
      ## Print this help and exit.
      -h|--help) doc --print-help "$0"; exit 0 ;;
    esac
    shift
  done
}

## \usage {{cookiecutter.package_name}} [-h]
main "$@"
